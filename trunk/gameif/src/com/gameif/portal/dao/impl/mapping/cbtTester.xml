<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="cbtTester">
	<typeAlias alias="cbtTester" type="com.gameif.portal.entity.CbtTester" />
	
	<resultMap class="com.gameif.portal.entity.CbtTester" id="cbtTesterResult">
		<result property="memNum" column="mem_num" />
		<result property="titleId" column="title_id" />
	</resultMap>

	<insert id="save" parameterClass="cbtTester">
		<![CDATA[
			INSERT INTO cbt_tester
				(
				mem_num,
				title_id)
			VALUES
				(
				#memNum#,
				#titleId#
				)
		]]>
	</insert>
	
	<delete id="deleteByKey" parameterClass="cbtTester">  
        <![CDATA[
	        DELETE
			FROM
				cbt_tester
			WHERE
				mem_num = #memNum#
			AND title_id = #titleId#
        ]]>
	</delete>

</sqlMap>
