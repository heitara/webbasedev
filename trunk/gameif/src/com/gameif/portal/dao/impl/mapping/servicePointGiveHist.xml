				point_amount,
				created_date,
				created_user,
				last_update_date,
				last_update_user)
			VALUES
				(#servicePointNo#,
				#servicePointTypeId#,
				#titleId#,
				#giveDate#,
				#pointStartDate#,
				#pointEndDate#,
				#pointAmount#,
