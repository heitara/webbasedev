<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="titleMst">
	<typeAlias alias="titleMst" type="com.gameif.portal.entity.TitleMst" />

	<resultMap class="com.gameif.portal.entity.TitleMst" id="titleResult">
		<result property="titleId" column="title_id" />
		<result property="titleName" column="title_name" />
		<result property="validStartDate" column="valid_start_date" />
		<result property="validEndDate" column="valid_end_date" />
		<result property="siteUrl" column="site_url" />
		<result property="smallIconUrl" column="small_icon_url" />
		<result property="bigIconUrl" column="big_icon_url" />
		<result property="createdDate" column="created_date"/>
		<result property="createdUser" column="created_user"/>
		<result property="lastUpdateDate" column="last_update_date"/>
		<result property="lastUpdateUser" column="last_update_user"/>
	</resultMap>

	<select id="selectValidTitleList" resultMap="titleResult">   
        <![CDATA[
        SELECT
			title_id,
			title_name,
			valid_start_date,
			valid_end_date,
			site_url,
			small_icon_url,
			big_icon_url,
			created_date,
			created_user,
			last_update_date,
			last_update_user
		FROM
			title_mst
		WHERE
			valid_start_date >= now()
		AND valid_end_date <= now()
        ]]>
	</select>

</sqlMap>