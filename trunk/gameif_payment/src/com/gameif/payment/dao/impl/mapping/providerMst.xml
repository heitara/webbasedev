<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="providerMst">

	<resultMap class="com.gameif.payment.entity.ProviderMst" id="providerResult">
		<result property="providerId" column="provider_id" />
		<result property="providerName" column="provider_name" />
		<result property="providerKindCd" column="provider_kind_cd" />
		<result property="createdDate" column="created_date"/>
		<result property="createdUser" column="created_user"/>
		<result property="lastUpdateDate" column="last_update_date"/>
		<result property="lastUpdateUser" column="last_update_user"/>
	</resultMap>

	<select id="selectByKey" parameterClass="com.gameif.payment.entity.ProviderMst" resultMap="providerResult">   
        <![CDATA[
		SELECT	provider_id,
				provider_name,
				provider_kind_cd,
				created_date,
				created_user,
				last_update_date,
				last_update_user
		FROM	provider_mst
		WHERE	provider_id = #providerId#
        ]]>
	</select>

</sqlMap>