<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="inquiryKindMst">
	<resultMap class="com.gameif.payment.entity.InquiryKindMst" id="inquiryKindResult">
		<result property="inquiryKindCode" column="inquiry_kind_code" />
		<result property="inquiryKindName" column="inquiry_kind_name" />
		<result property="createdDate" column="created_date"/>
		<result property="createdUser" column="created_user"/>
		<result property="lastUpdateDate" column="last_update_date"/>
		<result property="lastUpdateUser" column="last_update_user"/>
	</resultMap>

	<select id="selectAll" resultMap="inquiryKindResult">   
        <![CDATA[
        SELECT
			inquiry_kind_code,
			inquiry_kind_name,
			created_date,
			created_user,
			last_update_date,
			last_update_user
		FROM
			inquiry_kind_mst
        ]]>
	</select>

</sqlMap>